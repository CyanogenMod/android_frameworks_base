    <!-- Privacy Guard -->
    <string name="permlab_changePrivacyGuardState">"தனியுரிமை பாதுகாப்பு செயல்படுத்த அல்லது செயல்நீக்க"</string>
    <string name="permdesc_changePrivacyGuardState">"பயன்பாட்டை மற்றொரு பயன்பாடு தனியுரிமை பாதுகாப்பு கொண்டு இயங்குமா என்பதை மாற்ற அனுமதிக்கிறது. ஒரு பயன்பாடு தனியுரிமை பாதுகாப்பு கொண்டு இயங்கும் போது, இது போன்ற தொடர்புகள், அழைப்பு பதிவுகள், மற்றும் செய்திகள் போன்ற தனிப்பட்ட தரவுக்கான அணுகல் இருக்காது."</string>
    <string name="privacy_guard_notification">"தனியுரிமை பாதுகாப்பு செயலில்"</string>
    <string name="privacy_guard_notification_detail"><xliff:g id="app">%1$s</xliff:g> தனிப்பட்ட தரவை அணுக முடியாது</string>

