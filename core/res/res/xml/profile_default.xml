<profiles>
    <profile nameres="profileNameDefault">
    </profile>
    <profile nameres="profileNameSilent">
        <ringModeDescriptor>
            <value>mute</value>
            <override>true</override>
        </ringModeDescriptor>
    </profile>
    <profile nameres="profileNameAutomobile">
        <!-- turn GPS on -->
        <connectionDescriptor>
            <connectionId>4</connectionId>
            <value>1</value>
            <override>true</override>
        </connectionDescriptor>
        <!-- turn Bluetooth on -->
        <connectionDescriptor>
            <connectionId>7</connectionId>
            <value>1</value>
            <override>true</override>
        </connectionDescriptor>
    </profile>
</profiles>
