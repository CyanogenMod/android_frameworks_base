<?xml version="1.0" encoding="utf-8"?>
<policy>

  <!-- Media dev key in AOSP -->
  <signer signature="" >
    <allow-all />
    <seinfo value="media" />
  </signer>

  <package name="com.android.frameworks.coretests.mmac_install_platform" >
    <deny-permission name="android.permission.SET_ANIMATION_SCALE" />
    <deny-permission name="android.permission.CHANGE_CONFIGURATION" />
    <seinfo value="package" />
  </package>

  <default>
    <allow-all />
    <seinfo value="default" />
  </default>

</policy>
