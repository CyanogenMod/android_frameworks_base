    ralt:                               '8'
    ralt+shift:                         '*'
}

key 9 {
    label:                              '\u00bb'
    base:                               '\u00bb'
    shift:                              ')'
    ralt:                               '9'
    ralt+shift:                         '('
}

key 0 {
    label:                              '\u0555'
    base:                               '\u0585'
    shift, capslock:                    '\u0555'
    ralt:                               '0'
    ralt+shift:                         ')'
}

key MINUS {
    label:                              '\u054c'
